{{#partial "content"}}
    <h1 class="title1"><u>Spotted Animals</u></h1><br>
    <div class="container">
        <div class="row">
            <div class="col-md-6">
                <h3><u>Non-endangered Animals</u></h3>
                <div class="thumbnail">
                    <center>
                        <h4>.getName()</h4>
                        <h4>.getLocation()</h4>
                        <h4>.getRanger()</h4>
                    </center>
                </div>
            </div>
            <div class="col-md-6">
                <h3><u>Endangered Animals</u></h3>
                <div class="thumbnail">
                    <center>
                        <h4>.getName()</h4>
                        <h4>.getHealth()</h4>
                        <h4>.getAge()</h4>
                        <h4>.getLocation()</h4>
                        <h4>.getRanger()</h4>
                    </center>
                </div>
            </div>
        </div>
    </div>


{{/partial}}

{{> layout.hbs}}